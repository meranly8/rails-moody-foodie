<h1><%= @entry.user.name %> Entry for <%= @entry.date %></h1>
<ul>
    <% @entry.entry_provisions.each do |ep| %>
        <li><%= link_to ep.provision.name, provision_path(ep.provision) %> (<%= ep.provision.provision_type %>)<br>
        <%= ep.amount %> <%= ep.unit %></li> 
    <% end %>        
</ul>